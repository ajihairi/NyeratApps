<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.polban.kota111.nyerat">
    <!--android:name=".NyeratApp"-->
    <application
    android:name=".NyeratApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity android:name=".activities.TampilanLatihanActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/tittle_activity_tampilan_latihan"
            android:theme="@style/temalatihan"/>

        <activity android:name=".activities.MainLatihanActivity" />
        <activity android:name=".activities.MainMenuActivity" />


        <activity
            android:name=".activities.SplashScreenActivity"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:label="@string/title_activity_splash_screen"
            android:theme="@style/FullscreenTheme"/>

        <activity
            android:name=".activities.AboutActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_about"
            android:theme="@style/temalatihan">

        </activity>
    </application>

</manifest>